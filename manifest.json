{
    "manifest_version": 2,
    "name": "Intra2outlook",
    "version": "1.0",
    "description": "Send event email when register to an epitech activity",
    "content_scripts": [{
        "matches": ["https://intra.epitech.eu/*"],
        "js": ["index.js"]
      }],
      "icons": {
        "16": "logo_16x16.png",
        "48": "logo_48x48.png",
        "128": "logo_128x128.png"
      }
  }